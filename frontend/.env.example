# Backend API Configuration
REACT_APP_BACKEND_URL=http://localhost:8000

# Application Configuration
REACT_APP_API_VERSION=v1
REACT_APP_APP_NAME="Nite Putter Pro"
REACT_APP_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_CHAT_SUPPORT=false
REACT_APP_ENABLE_BETA_FEATURES=false

# External Services
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key
REACT_APP_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Environment
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG=true

# CDN & Assets
REACT_APP_CDN_URL=https://cdn.niteputter.com
REACT_APP_IMAGES_BASE_URL=https://images.niteputter.com